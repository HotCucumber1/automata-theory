digraph Mealy {
    __initial__ [shape=point];
    __initial__ -> A;

    A [shape=circle];
    B [shape=circle];
    C [shape=circle];
    D [shape=circle];
    E [shape=circle];
    F [shape=circle];
    G [shape=circle];

    A -> B [label="x0 / y1"];
    A -> C [label="x1 / y1"];

    B -> D [label="x0 / y0"];
    B -> F [label="x1 / y2"];

    C -> E [label="x0 / y0"];
    C -> F [label="x1 / y2"];

    D -> A [label="x0 / y1"];
    D -> D [label="x1 / y0"];

    E -> A [label="x0 / y1"];
    E -> D [label="x1 / y0"];

    F -> B [label="x0 / y2"];
    F -> C [label="x1 / y2"];

    G -> D [label="x1 / y2"];
}